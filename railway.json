{
  "build": {
    "builder": "Nixpacks",
    "buildCommand": "pip install -r requirements.txt"
  },
  "services": [
    {
      "name": "web",
      "startCommand": "sh -c "python app.py & python python/main.py"",
      "env": {
        "FLASK_ENV": "production"
      },
      "port": 8000
    }
  ]
}
